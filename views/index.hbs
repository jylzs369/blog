<main>
    <section class="article">
        {{# each posts}}
        <article>
            <h3 class="title">{{title}}</h3>
            <div class="label"><span class="author">{{../user.name}}</span><span class="time">{{time}}</span></div>
            <p class="brief">{{content}}</p>
        </article>
        {{/each}}
    </section>
</main>